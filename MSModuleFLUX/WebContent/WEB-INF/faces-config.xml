<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">

	<application>
		<locale-config>
			<default-locale>fr</default-locale>
		</locale-config>
		<resource-bundle>
			<base-name>org.metabosite.module.flux.bundles.MODULE</base-name>
			<var>MODULE_RB</var>
		</resource-bundle>
		<resource-bundle>
			<base-name>org.metabosite.module..flux.bundles.UI_FILE_SELECTOR</base-name>
			<var>UI_FS_RB</var>
		</resource-bundle>
	</application>
</faces-config>
